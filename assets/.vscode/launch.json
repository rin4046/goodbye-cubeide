{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "STM32 Debug",
      "request": "launch",
      "type": "cortex-debug",
      "cwd": "${workspaceFolder}",
      "executable": "./Debug/${workspaceFolderBasename}.elf",
      "armToolchainPath": "${command:goodbye-cubeide.toolchainPath}",
      "preLaunchTask": "STM32 Build",
      "runToEntryPoint": "main",
      "servertype": "stlink",
      "serverpath": "${command:goodbye-cubeide.stlinkPath}",
      "stm32cubeprogrammer": "${command:goodbye-cubeide.cubeProgPath}"
    }
  ]
}
